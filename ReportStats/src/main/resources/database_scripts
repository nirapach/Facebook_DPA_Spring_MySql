CREATE SCHEMA G4_DPA_REPORTS_DB;

CREATE TABLE `G4_DPA_REPORTS_DB`.`Account_Information_Master` (
`Application_Client_ID` BIGINT NULL COMMENT '',
`Business_Manager_Ad_Account_ID` BIGINT NULL COMMENT '',
`Business_Manager_Account_Username` VARCHAR(45) NULL COMMENT '',
`Application_Long_Lived_Access_Token` LONGTEXT NULL COMMENT '');

CREATE TABLE `G4_DPA_REPORTS_DB`.`Product_Account_Statistics_Results` (
`Application_Client_ID` BIGINT NULL COMMENT '',
`Application_Ad_Account_ID` BIGINT NULL COMMENT '',
`Client_Product_ID` BIGINT NULL COMMENT '',
`Client_Reports_Age_Stats_Start_Range` INT NULL COMMENT '',
`Client_Reports_Age_Stats_End_Range` INT NULL COMMENT '',
`Client_Reports_Country_Stats` VARCHAR(45) NULL COMMENT '',
`Client_Reports_Gender_Stats` VARCHAR(45) NULL COMMENT '',
`Client_Reports_Placement` VARCHAR(45) NULL COMMENT '',
`Client_Reports_Impression_Device` VARCHAR(45) NULL COMMENT '',
`Client_Reports_Reach` BIGINT NULL COMMENT '',
`Client_Reports_Frequency` DOUBLE NULL COMMENT '',
`Client_Reports_Impressions` BIGINT NULL COMMENT '',
`Client_Reports_Clicks` BIGINT NULL COMMENT '',
`Client_Reports_Total_Actions` BIGINT NULL COMMENT '',
`Client_Reports_Social_Reach` BIGINT NULL COMMENT '',
`Client_Reports_Social_Impressions` BIGINT NULL COMMENT '',
`Client_Reports_Unique_Impressions` BIGINT NULL COMMENT '',
`Client_Reports_Unique_Social_Impressions` INT NULL COMMENT '',
`Client_Reports_CPM` DOUBLE NULL COMMENT '',
`Client_Reports_CPP` DOUBLE NULL COMMENT '',
`Client_Reports_CPC` DOUBLE NULL COMMENT '',
`Client_Reports_CTR` DOUBLE NULL COMMENT '',
`Client_Reports_Spend` DOUBLE NULL COMMENT '',
`Stats_Date` DATETIME NULL COMMENT '',
`Client_Reports_Ad_Activity_Date_Start` DATETIME NULL COMMENT '',
`Client_Reports_Ad_Activity_Date_End` DATETIME NULL COMMENT '');

