  `Client_Reports_Age_Stats_End_Range` int(11) DEFAULT NULL,
  `Client_Reports_Gender_Stats` varchar(45) DEFAULT NULL,
  `Client_Reports_Reach` bigint(20) DEFAULT NULL,
  `Client_Reports_Frequency` double DEFAULT NULL,
  `Client_Reports_Impressions` bigint(20) DEFAULT NULL,
  `Client_Reports_Clicks` bigint(20) DEFAULT NULL,
  `Client_Reports_Total_Actions` bigint(20) DEFAULT NULL,
  `Client_Reports_Social_Reach` bigint(20) DEFAULT NULL,
  `Client_Reports_Social_Impressions` bigint(20) DEFAULT NULL,
  `Client_Reports_Unique_Impressions` bigint(20) DEFAULT NULL,
  `Client_Reports_Unique_Social_Impressions` int(11) DEFAULT NULL,
  `Client_Reports_CPM` double DEFAULT NULL,
  `Client_Reports_CPP` double DEFAULT NULL,
  `Client_Reports_CPC` double DEFAULT NULL,
  `Client_Reports_CTR` double DEFAULT NULL,
  `Client_Reports_Spend` double DEFAULT NULL,
  `Stats_Date` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `Client_Reports_Ad_Activity_Date_Start` datetime DEFAULT NULL,
  `Client_Reports_Ad_Activity_Date_End` datetime DEFAULT NULL,
  `Client_Cost_Per_Unique_Click` double DEFAULT NULL,
  PRIMARY KEY (`Application_Client_ID`,`Stats_Date`));

CREATE TABLE `Overall_Ad_Statistics_Results` (
